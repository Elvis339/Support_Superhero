{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "warn",
        "no-underscore-dangle": "off",
        "array-callback-return": "off",
        "func-names": "warn",
        "consistent-return": "off"
    }
}